{
  "name": "api",
  "private": true,
  "dependencies": {
    "jsonschema": "1.2.4",
    "jsonwebtoken": "^8.4.0",
    "knex": "0.15.2",
    "koa": "2.6.1",
    "koa-bodyparser": "4.2.1",
    "koa-mount": "^4.0.0",
    "koa-router": "7.4.0",
    "koa-static": "5.0.0",
    "lodash": "4.17.11",
    "mysql2": "1.6.4",
    "pg": "7.7.1",
    "sqlite3": "4.0.3",
    "uuid": "3.3.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.ts"
    ]
  },
  "devDependencies": {
    "@types/jest": "23.3.9",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/knex": "0.15.0",
    "@types/koa": "2.0.46",
    "@types/koa-bodyparser": "5.0.1",
    "@types/koa-mount": "^3.0.1",
    "@types/koa-router": "7.0.33",
    "@types/koa-send": "^4.1.1",
    "@types/koa-static": "^4.0.0",
    "@types/lodash": "4.14.118",
    "@types/mockdate": "^2.0.0",
    "@types/node": "10.12.5",
    "@types/supertest": "^2.0.6",
    "@types/uuid": "3.4.4",
    "jest": "^23.6.0",
    "mockdate": "^2.0.2",
    "nodemon": "^1.18.6",
    "prettier": "^1.15.3",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.4",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "typescript": "^3.1.6"
  },
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all",
    "printWidth": 80
  }
}
