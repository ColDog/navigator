{
  "name": "example",
  "chart": "github.com/ColDog/navigator.git//deployer/charts/service",
  "deploy": "./deployer-mock.sh",
  "stages": [
    {
      "name": "review",
      "review": true,
      "auto": true,
      "clusters": [
        {
          "name": "minikube",
          "namespace": "default",
          "values": {
            "image": {
              "repository": "coldog/nginx"
            }
          }
        }
      ]
    },
    {
      "name": "staging",
      "promote": true,
      "auto": true,
      "clusters": [
        {
          "name": "minikube",
          "namespace": "default",
          "values": {
            "image": {
              "repository": "coldog/nginx"
            }
          }
        }
      ]
    },
    {
      "name": "production",
      "auto": false,
      "clusters": [
        {
          "name": "minikube",
          "namespace": "default",
          "values": {
            "image": {
              "repository": "coldog/nginx"
            }
          }
        }
      ]
    }
  ]
}
